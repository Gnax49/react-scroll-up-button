{
  "name": "react-scroll-up-button",
  "version": "1.6.3",
  "main": "dist/react-scroll-up-button.js",
  "description": "React component Scroll up button, fixed button to scrolll up to desired position",
  "scripts": {
    "compile": "npx babel-cli -d dist/ src/",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev:server": "npx webpack-dev-server --config ./UITesting/webpack.config.js --hot --inline",
    "dev:cypress": "start-server-and-test dev:server http://localhost:8080 cypress:open",
    "dev:test": "npx jest --coverage --watch",
    "test": "npx jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:server": "npm run compile && npx webpack-dev-server --config ./UITesting/webpack.testing.js",
    "test:cypress": "start-server-and-test test:server http://localhost:8080 cypress:run",
    "prepublishOnly": "npm run compile"
  },
  "jest": {
    "verbose": true,
    "testRegex": "(/tests/.*)",
    "globals": {
      "window": true
    },
    "setupFiles": [
      "jest-prop-type-error"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,jsx}"
    ]
  },
  "author": "Dirtyredz | David McCLain <david.mcclain@dirtyredz.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dirtyredz/react-scroll-up-button.git"
  },
  "keywords": [
    "scroll",
    "scrollUp",
    "scrollToTop",
    "animation",
    "effects",
    "react",
    "react-component"
  ],
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.2",
    "cypress": "^3.1.0",
    "jest": "^23.6.0",
    "jest-dom": "^1.12.0",
    "jest-prop-type-error": "^1.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-testing-library": "^5.0.1",
    "start-server-and-test": "^1.7.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "peerDependencies": {
    "react": "^15.5.4 || ^16.0.0"
  },
  "dependencies": {
    "detect-passive-events": "^1.0.4",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.2",
    "tween-functions": "^1.2.0"
  }
}
